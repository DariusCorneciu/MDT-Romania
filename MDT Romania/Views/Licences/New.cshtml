@model MDT_Romania.Models.Licence


<form method="post" action="/Licences/New">

    <input type="hidden" name="CivilianId" value="@Model.CivilianId" />
    <input type="hidden" name="LicenceType" value="@Model.LicenceType" />
            <div data-mdb-input-init class="form-outline">
        @Html.ValidationMessage("Relesedby", null, new { @class = "text-danger" })
        <input type="text" id="form6Example1" class="form-control" name="Relesedby" value="@Model.Relesedby" />
        <label class="form-label" for="form6Example1">Relesed by</label>

            </div>

     <div data-mdb-input-init class="form-outline mb-4">
        @Html.ValidationMessage("PermitNumber", null, new { @class = "text-danger" })
        <input type="text" id="form6Example1" class="form-control" name="PermitNumber" value="@Model.PermitNumber" />
        <label class="form-label" for="form6Example1">Permit Number</label>
    </div>
    <div data-mdb-input-init class="form-outline mb-4">
        @Html.ValidationMessage("subcateg", null, new { @class = "text-danger" })
        <input type="text" id="form6Example1" class="form-control" name="subcateg" value="@Model.subcateg" />
        <label class="form-label" for="form6Example1">Sub category</label>
    </div>

           
       
       
    <button data-mdb-ripple-init type="submit" class="btn btn-primary btn-block mb-4 mt-4">Give Licence</button>
</form>

